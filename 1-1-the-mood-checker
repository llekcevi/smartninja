mood = input("How are you feeling?")

if mood == "happy":
    print("It is great to see you happy!")
elif mood == "nervous":
    print("Take a deep breath 3 times.")
elif mood == "sad":
    print("Cheer up, mate!")
elif mood == "excited":
    print("Great!")
elif mood == "relaxed":
    print("Enjoy!")
else:
    print("I don't recognize this mood"
